{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "browserslist": "last 1 versions, > 0.2%, not dead",
  "scripts": {
    "build:client:dev": "webpack --config webpack.client.config.js --mode development",
    "build:client:prod": "webpack --config webpack.client.config.js --mode production",
    "webpack:bundle:analyzer:dev": "NODE_ENV=analyse npm run build:client:dev",
    "webpack:bundle:analyzer:prod": "NODE_ENV=analyse npm run build:client:prod",
    "start": "NODE_ENV=hmr webpack-dev-server --config webpack.client.config.js --mode development --port 3000",
    "memstart": "NODE_ENV=hmr node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client.config.js --mode development --inline --progress --port 3000"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.2.2",
    "emotion": "^10.0.9",
    "emotion-server": "^10.0.9",
    "express": "^4.16.4",
    "express-http-context": "^1.2.0",
    "focus-trap-react": "^5.0.1",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "node-fetch": "^3.1.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.0.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.6",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "extract-css-chunks-webpack-plugin": "^3.3.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-html-parser": "^1.1.12",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "^4.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
